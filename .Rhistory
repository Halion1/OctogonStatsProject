test1<- rbind(x,y)
mp <- barplot(test1, beside = TRUE, xlab = "Cantidad de Dinero gastado", ylab = "Frecuencia")
text(mp,test1, labels = format(test1,4), pos=3, cex=.75)
legend(6,7,legend=c("frecuencia de No Atletas","Frecuencia de Atletas"),col=c("black","grey"),lwd=2)
